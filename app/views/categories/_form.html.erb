<%= form_for @category do |form| %>
    <%= render 'shared/error_messages', object: form.object %>
    <table>
        <tr>
            <td><%= form.label :name %></td>
            <td><%= form.text_field :category, size: 22 %></td>
        </tr>
        <tr>
            <td><%= form.submit title_sumbit %></td>
        </tr>
    </table>
<% end %>

<div><%= button_to 'Back', categories_path, method: :get %></div>
